<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>com.apple.private.backgroundtaskmanagement.manage</key>
	<true/>
	<key>com.apple.private.ctk.slot-type</key>
	<string>smartcard</string>
	<key>com.apple.private.tcc.allow</key>
	<array>
		<string>kTCCServiceSystemPolicySysAdminFiles</string>
	</array>
	<key>com.apple.security.app-sandbox</key>
	<true/>
	<key>com.apple.security.device.usb</key>
	<true/>
	<key>com.apple.security.iokit-user-client-class</key>
	<array>
		<string>AppleUSBHostFrameworkInterfaceClient</string>
	</array>
	<key>com.apple.security.temporary-exception.files.absolute-path.read-only</key>
	<array>
		<string>/usr/libexec/SmartCardServices/drivers/</string>
		<string>/usr/local/libexec/SmartCardServices/drivers/</string>
	</array>
	<key>com.apple.security.temporary-exception.mach-lookup.global-name</key>
	<array>
		<string>com.apple.ctkd.slot-registry</string>
		<string>com.apple.backgroundtaskmanagement</string>
	</array>
	<key>com.apple.security.temporary-exception.sbpl</key>
	<array>
		<string>(allow user-preference-read user-preference-write (preference-domain "com.apple.security.smartcard"))</string>
		<string>(allow iokit-open (iokit-user-client-class "AppleUSBHostFrameworkInterfaceClient"))</string>
	</array>
</dict>
</plist>
